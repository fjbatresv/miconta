
{% block list_row %}
<tr class="list_trow">{% block list_row_content %}
                      <td class="td_VARCHAR td_username">
              {% block list_td_column_username %}{{ Usuario.username }}{% endblock list_td_column_username %}
          </td>
                      <td class="td_VARCHAR td_email">
              {% block list_td_column_email %}{{ Usuario.email }}{% endblock list_td_column_email %}
          </td>
        {% endblock list_row_content %}
    {% block object_actions %}
                                <td class="object_actions">
                            <div class="btn-group">
                                    {% block object_action_edit %}
                            
                                        
    {% spaceless %}
    <a  class="btn " href="{{ path("MiConta_SoporteBundle_Usuario_edit", { pk: Usuario.Id }) }}"
        title="{% trans from "Admingenerator" %}action.object.edit.label{% endtrans %}"
                rel="tooltip"
        ><i class="fa fa-edit"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_edit %}
                                    {% block object_action_perfiles %}
                            
                                        
    {% spaceless %}
    <a  class="btn " href="{{ path("editar_perfiles_usuario", { 'pk': Usuario.Id, action: 'perfiles' }) }}"
        title="{% trans from "Admin" %}Perfiles{% endtrans %}"
                rel="tooltip"
        ><i class="fa fa fa-sitemap"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_perfiles %}
                                    {% block object_action_delete %}
                            
                                        
    {% spaceless %}
    <a  class="btn " href="{{ path("MiConta_SoporteBundle_Usuario_object", { pk: Usuario.Id, action: 'delete' }) }}"
        title="{% trans from "Admingenerator" %}action.object.delete.label{% endtrans %}"
                rel="tooltip"
         data-confirm="{% trans from "Admingenerator" %}action.object.delete.confirm{% endtrans %}"  data-csrf-token="{{ path("MiConta_SoporteBundle_Usuario_object", { pk: Usuario.Id, action: 'delete' })|csrf_token }}" ><i class="fa fa-times"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_delete %}
                                {% block object_actions_customActions %}{% endblock object_actions_customActions %}
                </div>
                        </td>
                    {% endblock object_actions %}

</tr>
{% endblock list_row %}
